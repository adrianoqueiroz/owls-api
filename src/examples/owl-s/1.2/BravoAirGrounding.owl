        <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">&BravoAirGroundingWSDL;#arrivalAirport</grounding:wsdlMessagePart>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="&ba_process;#GetDesiredFlightDetails_OutboundDate"/>
        <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">&BravoAirGroundingWSDL;#outboundDate</grounding:wsdlMessagePart>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="&ba_process;#GetDesiredFlightDetails_InboundDate"/>
        <grounding:wsdlMessagePart rdf:datatype="&xsd;#anyURI">&BravoAirGroundingWSDL;#inboundDate</grounding:wsdlMessagePart>
